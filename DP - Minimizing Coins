void jai_bajrangbali()
{
    ll k,n,m,x,y,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n>>sum;
    ll a[n];
    lala
    {
        cin>>a[i];
    }


//    vi tt(sum+1,1e9);
//    tt[0]=0;

    ll tt[sum+1];
    for(ll i=0;i<sum+1;i++)
    {
        tt[i]=1e9;
    }


    tt[0]=0;

    for(ll i=0;i<sum+1;i++)
    {
        for(ll j=0;j<n;j++)
        {
            if(i-a[j]>= 0)
            {
                tt[i]=min(tt[i],1 + tt[i-a[j]]);
            }
        }
    }

    ct<<(tt[sum]==1e9 ? -1 : tt[sum]) <<ed;


}

int main()
{
    bahut_tezz();
    long long t=1;
    //cin>>t;
    while(t--)jai_bajrangbali();

    return 0;
}

