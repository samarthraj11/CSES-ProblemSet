void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n];
    if(n==1)
    {
        ct<<1<<ed;
    }
    else if(n<=3)
    {
        ct<< "NO SOLUTION"<<ed;
 
    }
    else
    {
        vi v;
        for(ll i=1;i<=n;i++)
        {
            if(i%2==0)
            {
                v.pb(i);
            }
        }
        for(ll i=1;i<=n;i++)
        {
            if(i%2 != 0)
            {
                v.pb(i);
            }
        }
        for(ll i=0;i<v.sz;i++)
        {
            ct<<v[i]<< " ";
        }
 
    }
}
 
int main()
