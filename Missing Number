using namespace std;
ll till_sum(ll n)
{
    ll sum=0;
    for(ll i=1;i<=n;i++)
    {
        sum=sum+i;
    }
    return sum;
 
}
 
void bahut_tezz()
{
    std::ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}
 
 
void jai_bajrangbali()
{
    ll c,d,e,n,k,x,y,z,cnt=0,res=0,mx=1;
    ll sum=0;
    bool ash=false;
    string s;
    cin>>n;
    ll a[n-1];
    for(ll i=0;i<n-1;i++)
    {
        cin>>a[i];
        cnt=cnt+a[i];
    }
    ct<<till_sum(n)-cnt<<ed;
}
 
int main()
{
    bahut_tezz();
    long long t;
    //cin>>t;
    t=1;
    while(t--)
    {
 
        jai_bajrangbali();
 
 
    }
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
